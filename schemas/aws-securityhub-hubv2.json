{
  "$id": "aws-securityhub-hubv2",
  "$integrity": "sha512-W9ypFEXEgDM+Ho/TaF6zVQ0D9q4gsKZozgndSSCpezqMj+WkwcQZyFigeYUde0eg7y88DZsMgI6vEthyXenDXg==",
  "additionalProperties": false,
  "definitions": {
    "ISO8601DateString": {
      "pattern": "^(\\d\\d\\d\\d)-([0][1-9]|[1][0-2])-([0][1-9]|[1-2](\\d)|[3][0-1])[T](?:([0-1](\\d)|[2][0-3]):[0-5](\\d):[0-5](\\d)|23:59:60)(?:\\.(\\d)+)?([Z]|[+-](\\d\\d)(:?(\\d\\d))?)$",
      "type": "string"
    },
    "Tags": {
